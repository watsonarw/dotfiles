{
  "[dotenv]": {},
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "FiraCode Nerd Font, Fira Code, Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    120
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.stablePeek": true,
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editorconfig.generateAuto": true,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.Brewfile": "ruby"
  },
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.views.branches.branches.layout": "list",
  "json.schemas": [],
  "launch": {
    "compounds": [],
    "configurations": []
  },
  "markdown-mermaid.darkModeTheme": "base",
  "markdown-mermaid.lightModeTheme": "base",
  "markdown-preview-github-styles.colorTheme": "light",
  "notebook.stickyScroll.enabled": true,
  "prettier.configPath": ".prettierrc",
  "shellformat.useEditorConfig": true,
  "svg.preview.mode": "img",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 100000,
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "diffEditor.renderSideBySide": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off"
}
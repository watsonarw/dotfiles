{
  "[dotenv]": {},
  "cSpell.language": "en-AU,en",
  "cSpell.userWords": [],
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.fontFamily": "FiraCode Nerd Font, Fira Code, Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "editor.scrollBeyondLastLine": false,
  "editor.stablePeek": true,
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editorconfig.generateAuto": true,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.Brewfile": "ruby"
  },
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "mdx": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "gitlens.views.branches.branches.layout": "list",
  "json.schemas": [],
  "launch": {
    "compounds": [],
    "configurations": []
  },
  "markdown-mermaid.darkModeTheme": "base",
  "markdown-mermaid.lightModeTheme": "base",
  "markdown-preview-github-styles.colorTheme": "light",
  "notebook.stickyScroll.enabled": true,
  "shellformat.useEditorConfig": true,
  "svg.preview.mode": "img",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.stickyScroll.enabled": false,
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "gitlens.rebaseEditor.openOnPausedRebase": false
}
